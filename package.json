{
  "name": "competitive-programming-platform",
  "version": "1.0.0",
  "description": "A comprehensive competitive programming platform with real-time contests and code execution",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:migrate": "node server/database/migrate.js",
    "db:seed": "node server/database/seed.js"
  },
  "keywords": [
    "competitive-programming",
    "leetcode",
    "codeforces",
    "judge0",
    "contests",
    "real-time"
  ],
  "author": "Competitive Programming Platform",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.0.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "marked": "^9.1.6",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "python-shell": "^5.0.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
